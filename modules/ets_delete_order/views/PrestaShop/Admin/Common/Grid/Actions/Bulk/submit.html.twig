{%if action.options.submit_route=='admin_order_bulk_delete'%}
    {%if ets_omd_is_viewtrash%}
        {%if ets_odm_can_delete_order%}
            <button id="order_grid_bulk_action_delete_selected" class="dropdown-item js-bulk-action-submit-btn" type="button" data-form-url="{{ets_odm_link_list_orders}}&delete_all_order=1&viewtrash=1" data-form-method="POST" data-confirm-message=""> {{Delete_selected_orders_text}} </button>
        {%endif%}
        <button id="order_grid_bulk_action_restore_selected" class="dropdown-item js-bulk-action-submit-btn" type="button" data-form-url="{{ets_odm_link_list_orders}}&restore_all_order=1&viewtrash=1" data-form-method="POST" data-confirm-message=""> {{Restore_selected_orders_text}} </button>
    {%else%}
        {%if ets_odm_can_delete_order%}
            <button id="order_grid_bulk_action_delete_selected" class="dropdown-item js-bulk-action-submit-btn" type="button" data-form-url="{{ets_odm_link_list_orders}}&delete_all_order" data-form-method="POST" data-confirm-message=""> {{Delete_selected_orders_text}} </button>
        {%endif%}
    {%endif%}
{%else%}
    {% include constant('_PS_VERSION_')|number_format >= 8  ? '@!PrestaShop/Admin/Common/Grid/Actions/Bulk/submit.html.twig': 'PrestaShopBundle:Admin/Common/Grid/Actions/Bulk:submit.html.twig' %}
{%endif%}